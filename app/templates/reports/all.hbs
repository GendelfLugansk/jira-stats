{{side-nav}}
<!-- CONTENT -->
<div id="content" data-uk-height-viewport="expand: true">
  <div class="uk-container uk-container-expand">

    {{issue-filters appliedFilters=parentController.filters onApplyFilters=(action (mut parentController.filters))
                    class="uk-width-1-1"}}

    <h3>Common</h3>

    <div class="uk-grid uk-grid-medium " uk-height-viewport="expand: true" data-uk-grid>

      {{reports/work-ratio-histogram filters=parentController.filters class="uk-width-1-1"}}

    </div>

    <h3 class="page-break-before">By Latest Sprint</h3>

    <div class="uk-grid uk-grid-medium " uk-height-viewport="expand: true" data-uk-grid>

      {{reports/median-work-ratio-by-latest-sprint filters=parentController.filters class="uk-width-1-2@xl"}}

      {{reports/mean-work-ratio-by-latest-sprint filters=parentController.filters class="uk-width-1-2@xl"}}

      {{reports/min-work-ratio-by-latest-sprint filters=parentController.filters class="uk-width-1-2@xl"}}

      {{reports/max-work-ratio-by-latest-sprint filters=parentController.filters class="uk-width-1-2@xl"}}

    </div>

    <h3 class="page-break-before">By Label</h3>

    <div class="uk-grid uk-grid-medium " uk-height-viewport="expand: true" data-uk-grid>

      {{reports/median-work-ratio-by-label filters=parentController.filters class="uk-width-1-2@xl"}}

      {{reports/mean-work-ratio-by-label filters=parentController.filters class="uk-width-1-2@xl"}}

      {{reports/min-work-ratio-by-label filters=parentController.filters class="uk-width-1-2@xl"}}

      {{reports/max-work-ratio-by-label filters=parentController.filters class="uk-width-1-2@xl"}}

    </div>

    <h3 class="page-break-before">By Labels (combined)</h3>

    <div class="uk-grid uk-grid-medium " uk-height-viewport="expand: true" data-uk-grid>

      {{reports/median-work-ratio-by-labels-combined filters=parentController.filters class="uk-width-1-2@xl"}}

      {{reports/mean-work-ratio-by-labels-combined filters=parentController.filters class="uk-width-1-2@xl"}}

      {{reports/min-work-ratio-by-labels-combined filters=parentController.filters class="uk-width-1-2@xl"}}

      {{reports/max-work-ratio-by-labels-combined filters=parentController.filters class="uk-width-1-2@xl"}}

    </div>

    {{x-footer}}
  </div>
</div>
<!-- /CONTENT -->