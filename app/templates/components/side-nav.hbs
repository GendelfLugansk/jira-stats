<div class="left-logo uk-flex uk-flex-middle">
  <img class="custom-logo"
       src="data:image/svg+xml;base64,"
       alt="">
  JIRA Stats
</div>
<div class="left-content-box  content-box-dark">
  <img
    src="data:image/svg+xml;base64,"
    alt="" class="uk-border-circle profile-img">
  <h4 class="uk-text-center uk-margin-remove-vertical text-light">Jon Snow</h4>

  <div class="uk-position-relative uk-text-center uk-display-block">
    <span class="uk-text-small uk-text-muted uk-display-block uk-text-center">You Know Nothing</span>
  </div>
</div>

<div class="left-nav-wrap">
  <ul class="uk-nav uk-nav-default uk-nav-parent-icon" data-uk-nav>
    <li class="uk-nav-header">ACTIONS</li>
    {{#link-to 'index' disabled=true tagName='li' activeClass='uk-active'}}
      {{#link-to 'index'}}
      <span data-uk-icon="icon: home" class="uk-margin-small-right"></span>Home
      {{/link-to}}
    {{/link-to}}

    {{#link-to 'import' disabled=true tagName='li' activeClass='uk-active'}}
      {{#link-to 'import'}}
        <span data-uk-icon="icon: upload" class="uk-margin-small-right"></span>Import
      {{/link-to}}
    {{/link-to}}

    {{#link-to 'issues' disabled=true tagName='li' activeClass='uk-active'}}
      {{#link-to 'issues'}}
        <span data-uk-icon="icon: table" class="uk-margin-small-right"></span>Closed Issues
      {{/link-to}}
    {{/link-to}}

    {{#link-to 'report' disabled=true tagName='li' activeClass='uk-active'}}
      {{#link-to 'report'}}
        <span data-uk-icon="icon: folder" class="uk-margin-small-right"></span>Report
      {{/link-to}}
    {{/link-to}}
  </ul>
</div>